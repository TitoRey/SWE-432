<div class="container">
  <div class="datatable-container">
    <h1>
      All Available Songs
    </h1>
    <table id="songs" class="table">
      <thead>
        <tr>
          <th></th>
          <th></th>
          <th></th>
        </tr>
        <tr>
          <th>Title</th>
          <th>Artist</th>
          <th>Genre</th>
        </tr>
      </thead>
      <tbody>
        <% songs.forEach(song=> { %>
          <tr>
            <td><%- song.title %></td>
            <td><%- song.artist %></td>
            <td><%- song.genre %></td>
          </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <div class="col-md-3">
      <a class="btn theme" href="/songs/new">New Song</a>
    </div>
  </div>
</div>